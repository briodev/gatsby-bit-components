{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "buildCSS": "postcss css/tailwind.css -o css/main.css",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "@mdx-js/react": "^1.5.1",
    "@theme-ui/prism": "^0.2.40",
    "@theme-ui/tailwind": "^0.2.41",
    "@theme-ui/typography": "^0.2.40",
    "gatsby": "^2.15.29",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-theme-style-guide": "^0.2.44",
    "react": "^16.10.2",
    "react-burger-menu": "^2.6.11",
    "react-dom": "^16.10.2",
    "tailwindcss": "^1.1.2",
    "theme-ui": "^0.2.44",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typography-theme-noriega": "^0.16.19"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.5",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
